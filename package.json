{
  "name": "vue-webpack",
  "version": "1.0.0",
  "description": "vue vue-router webpack 纯净版",
  "main": "./src/main.js",
  "scripts": {
    "init": "webpack --progress --config webpack.dev.config.js",
    "dev": "webpack --progress --colors --hide-modules --config  webpack.dev.config.js",
    "wl": "webpack --watch --progress --colors --hide-modules --config webpack.dev.config.js",
    "server": "webpack-dev-server --inline --hot --compress --history-api-fallback --config webpack.dev.config.js",
    "build": "webpack --progress --hide-modules --config webpack.prod.config.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/502222517/vue-webpack.git"
  },
  "author": "zhh",
  "license": "MIT",
  "dependencies": {
    "vue": "^2.2.2",
    "vue-router": "^2.3.0"
  },
  "devDependencies": {
    "autoprefixer-loader": "^3.2.0",
    "babel": "^6.23.0",
    "babel-core": "^6.23.1",
    "babel-loader": "^6.4.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-runtime": "^6.23.0",
    "css-loader": "^0.27.1",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.10.1",
    "html-loader": "^0.4.5",
    "html-webpack-plugin": "^2.28.0",
    "less": "^2.7.2",
    "less-loader": "^2.2.3",
    "node-sass": "^3.13.1",
    "sass-loader": "^4.1.1",
    "style-loader": "^0.13.2",
    "url-loader": "^0.5.8",
    "vue-hot-reload-api": "^2.0.11",
    "vue-html-loader": "^1.2.4",
    "vue-loader": "^7.5.3",
    "vue-style-loader": "^2.0.3",
    "webpack": "^2.2.1"
  },
  "bugs": {
    "url": "https://github.com/502222517/vue-webpack/issues"
  },
  "homepage": "https://github.com/502222517/vue-webpack#readme"
}
